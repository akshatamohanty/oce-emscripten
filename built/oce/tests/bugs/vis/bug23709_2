pcone a 5 0 10
box b 15 0 0 10 10 10
vinit
vdisplay a
vdisplay b
vrotate 35 0 0
vfit
vhlr on
vhlrtype algo a

set x_coord 290
set y_coord 170
checkcolor $x_coord $y_coord 1 1 0

set x_coord 86
set y_coord 221
checkcolor $x_coord $y_coord 0 0 0

set only_screen 1
